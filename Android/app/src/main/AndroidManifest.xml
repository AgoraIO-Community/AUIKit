<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.agora.app.sample">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sample.Voice"
        android:name=".SampleApplication"
        tools:replace="android:theme"
        tools:targetApi="31">
        <activity
            android:name="io.agora.app.sample.GuidanceActivity"
            android:label="GuidancePage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="基础组件"
            android:name=".BasicUiListActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            />

        <activity
            android:label="Components-voice"
            android:name=".VoiceCptListActivity"
            android:windowSoftInputMode="adjustNothing"
            android:exported="false"
            android:screenOrientation="portrait"
            />


    </application>

</manifest>